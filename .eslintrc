{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
		"commonjs": true,
		"jquery": true,
		"es6": true
  },
  "globals": {},
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-unused-expressions": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "quotes": [
      2,
      "double"
    ],
    "no-undef": 1,
    "no-unused-vars": 1
  },
  "parserOptions": {
    "ecmaVersion": 8
  }
}
