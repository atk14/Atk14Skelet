#: ../src/forms/fields/char_field.php:24 ../src/forms/fields/regex_field.php:25
msgid "Ensure this value has at most %max% characters (it has %length%)."
msgstr "Hodnota musí být dlouhá nejvýše %max% znaků (teď má %length%)."

#: ../src/forms/fields/char_field.php:25 ../src/forms/fields/regex_field.php:26
msgid "Ensure this value has at least %min% characters (it has %length%)."
msgstr "Hodnota musí mít alespoň %min% znaků (teď má %length%)."

#: ../src/forms/fields/char_field.php:26
msgid "Ensure this value has at most %max% characters."
msgstr "Tato hodnota může mít nejvýše %max% znaků."

#: ../src/forms/fields/char_field.php:27
msgid "Ensure this value has at least %min% characters."
msgstr "Hodnota musí mít alespoň %min% znaků."

#: ../src/forms/fields/char_field.php:28
msgid "Ensure this value has between %min% and %max% characters."
msgstr "Délka řetězce musí být mezi %min% a %max% znaky."

#: ../src/forms/fields/choice_field.php:37
msgid "Select a valid choice. That choice is not one of the available choices."
msgstr ""
"Vyberte platnou hodnotu. Vybraná hodnota není ani jedna z nabízených hodnot."

#: ../src/forms/fields/choice_field.php:38
msgid "Please, choose the right option."
msgstr "Prosím, vyberte správnou možnost."

#: ../src/forms/fields/date_field.php:17
msgid "Enter a valid date."
msgstr "Zadejde platné datum."

#: ../src/forms/fields/date_time_field.php:14
msgid "Enter a valid date, hours and minutes."
msgstr "Zadejte platné datum, hodiny a minuty."

#: ../src/forms/fields/date_time_with_seconds_field.php:14
msgid "Enter a valid date, hours, minutes and seconds."
msgstr "Zadejte platné datum, hodiny, minuty a sekundy."

#: ../src/forms/fields/email_field.php:20
msgid "Enter a valid e-mail address."
msgstr "Zadejte platnou e-mailovou adresu."

#: ../src/forms/fields/file_field.php:39
msgid "Ensure this file has at most %max_file_size% bytes (it has %file_size%)"
msgstr ""
"Ujistěte se, že velikost souboru nepřekročí %max_file_size% bytů (teď má "
"%file_size%)"

#: ../src/forms/fields/file_field.php:40
msgid "Unsupported file type: %mime_type%"
msgstr "Nepodporovaný typ souboru: %mime_type%"

#: ../src/forms/fields/file_field.php:42
msgid ""
"The uploaded file exceeds the max file size value that is set on the server "
"(%upload_max_filesize%)"
msgstr ""
"Nahraný soubor překračuje maximální velikost souboru, která je nastavena na "
"serveru (%upload_max_filesize%)"

#: ../src/forms/fields/file_field.php:43
msgid ""
"The uploaded file exceeds the max file size directive that was specified in "
"the HTML form (%MAX_FILE_SIZE%)"
msgstr ""
"Velikost nahraného souboru přesahuje maximální velikost, která je nastavena "
"v HTML formuláři (%MAX_FILE_SIZE%)"

#: ../src/forms/fields/file_field.php:44
msgid "The uploaded file was only partially uploaded"
msgstr "Nahraný soubor byl nahrán jen částečně"

#: ../src/forms/fields/file_field.php:45
msgid "Missing a temporary folder"
msgstr "Adresář pro dočasné soubory chybí"

#: ../src/forms/fields/file_field.php:46
msgid "Failed to write file to disk"
msgstr "Soubor se nepodařilo zapsat na disk"

#: ../src/forms/fields/file_field.php:47
msgid "File upload stopped by an extension installed on the server"
msgstr "Nahrávání souboru bylo zastaveno rozšířením nainstalovaným na serveru"

#: ../src/forms/fields/file_field.php:48
msgid "An error occurred during file upload"
msgstr "Během nahrávání souboru došlo k chybě"

#: ../src/forms/fields/float_field.php:22
msgid "Enter a number."
msgstr "Zadejte číslo."

#: ../src/forms/fields/float_field.php:23
#: ../src/forms/fields/integer_field.php:33
msgid "Ensure this value is less than or equal to %value%."
msgstr "Hodnota musí být rovna nebo  menší než %value%."

#: ../src/forms/fields/float_field.php:24
#: ../src/forms/fields/integer_field.php:34
msgid "Ensure this value is greater than or equal to %value%."
msgstr "Hodnota musí být rovna nebo větší než %value%."

#: ../src/forms/fields/image_field.php:22
msgid "Ensure this file is image."
msgstr "Ověřte, že tento soubor je obrázek."

#: ../src/forms/fields/image_field.php:24
msgid "Ensure this image is %width_required% pixels wide (it is %width%)."
msgstr ""
"Ujistěte se, že je šířka obrázku %width_required% bodů (teď je šířoký %width"
"%)."

#: ../src/forms/fields/image_field.php:25
msgid "Ensure this image is %height_required% pixels high (it is %height%)."
msgstr ""
"Ujistěte se, že výška obrázku je %height_required% bodů (teď je vysoký "
"%height%)."

#: ../src/forms/fields/image_field.php:27
msgid "Ensure this image is at most %max% pixels wide (it is %width%)."
msgstr ""
"Ujistěte se, že šířka obrázku nepřekročí %max% bodů (teď je šíroký %width%)."

#: ../src/forms/fields/image_field.php:28
msgid "Ensure this image is at most %max% pixels high (it is %height%)."
msgstr ""
"Ujistěte se, že výška obrázku nepřekročí %max% bodů (teď je vysoký %height%)."

#: ../src/forms/fields/image_field.php:29
msgid "Ensure this image is at least %min% pixels wide (it is %width%)."
msgstr "Obrázek musí mít alespoň %min% bodů na šířku (nyní má %width%). "

#: ../src/forms/fields/image_field.php:30
msgid "Ensure this image is at least %min% pixels high (it is %height%)."
msgstr "Obrázek musí mít alespoň %min% bodů na výšku (nyní má %height%). "

#: ../src/forms/fields/image_field.php:32
msgid "Ensure this image is in a required format"
msgstr "Ujistěte se, že je obrázek v požadovaném formátu"

#: ../src/forms/fields/integer_field.php:32
msgid "Enter a whole number."
msgstr "Zadejte celé číslo."

#: ../src/forms/fields/ip_address_field.php:26
msgid "Enter a valid IP address."
msgstr "Vložte platnou IP adresu."

#: ../src/forms/fields/multiple_choice_field.php:21
msgid "Select a valid choice. %value% is not one of the available choices."
msgstr "Vyberte platnou hodnotu. %value% není jedna z nabízených možností."

#: ../src/forms/fields/multiple_choice_field.php:22
msgid "Enter a list of values."
msgstr "Zadejte seznam hodnot."

#: ../src/forms/fields/multiple_choice_field.php:23
msgid "Please, choose the right options."
msgstr "Prosím, vyberte správné možnosti."

#: ../src/forms/fields.php:235
msgid "This field is required."
msgstr "Toto pole je povinné"

#: ../src/forms/fields.php:236
msgid "Enter a valid value."
msgstr "Vložta správnou hodnotu."

